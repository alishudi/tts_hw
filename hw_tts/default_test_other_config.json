{
  "name": "default_test_other_config",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000,
    "spectrogram": {
      "type": "MelSpectrogram",
      "args": {
        "n_mels": 160
      }
    },
    "log_spec": true
  },
  "arch": {
    "type": "DeepSpeech2",
    "args": {
      "n_feats": 160
    }
  },
  "data": {
    "test": {
      "batch_size": 30,
      "num_workers": 5,
      "datasets": [
        {
          "type": "LibrispeechDataset",
          "args": {
            "part": "test-other"
          }
        }
      ]
    }
  },
  "loss": {
    "type": "CTCLoss",
    "args": {}
  },
  "trainer": {
    "epochs": 1,
    "save_dir": "saved/",
    "save_period": 5,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 150,
    "visualize": "wandb",
    "wandb_project": "tts_project",
    "len_epoch": 1,
    "grad_norm_clip": 10
  }
}